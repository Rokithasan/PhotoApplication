<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.photoapplication.controller.AdminDashboard">
   <children>
      <Label alignment="CENTER" layoutX="168.0" layoutY="25.0" prefHeight="48.0" prefWidth="467.0" text="Welcome to Admin Page" textFill="#00b3ff">
         <font>
            <Font name="Times New Roman Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="73.0" prefHeight="33.0" prefWidth="205.0" text="All Users:">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </Label>
      <ListView fx:id="listUsers" layoutX="27.0" layoutY="106.0" prefHeight="413.0" prefWidth="301.0" />
      <Label layoutX="495.0" layoutY="117.0" prefHeight="33.0" prefWidth="205.0" text="Create New User" textFill="#00b3ff">
         <font>
            <Font name="Times New Roman Bold" size="23.0" />
         </font>
      </Label>
      <Label layoutX="430.0" layoutY="184.0" prefHeight="33.0" prefWidth="109.0" text="Username:">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="tfUsername" layoutX="539.0" layoutY="176.0" prefHeight="48.0" prefWidth="241.0" />
      <Button fx:id="btnCreate" layoutX="505.0" layoutY="257.0" mnemonicParsing="false" onAction="#actionOnCreate" prefHeight="33.0" prefWidth="160.0" text="Create" textFill="#1e6b5c">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnDelete" layoutX="27.0" layoutY="531.0" mnemonicParsing="false" onAction="#actionOnDelete" prefHeight="33.0" prefWidth="160.0" text="Delete" textFill="#1e6b5c">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="btnLogout" layoutX="693.0" layoutY="14.0" mnemonicParsing="false" onAction="#actionOnLogout" prefHeight="36.0" prefWidth="95.0" text="Logout" textFill="#1e6b5c">
         <font>
            <Font name="Times New Roman Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
